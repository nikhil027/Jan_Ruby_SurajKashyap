<h3>Edit : <%= @subject.subject_name %>

<%= form_for @subject do |f| %>
	<label> Name </label>
	<%= f.text_field :subject_name %>  <br/>
	<label> Instructor_ID </label>
	<%= f.text_field :instructor_id %>  <br/>
	<label> Descriptions</label>
	<%= f.text_area :descriptions %> <br/>
	
<%=f.submit "Submit" %>

<% end %>